property_value: isImplementedIn https://identifiers.org/biosimulators/copasi xsd:anyURI {comment="COPASI"}
property_value: seeAlso http://identifiers.org/doi/10.1145/321062.321069 xsd:anyURI {comment="R. Hooke and T. A. Jeeves. \"Direct search\" solution of numerical and statistical problems. Journal of the Association for Computing Machinery. 8. 212 - 229. 1961."}

